{
  "name": "openorchid",
  "productName": "OrchidBrowser",
  "manufacturer": "Orchid",
  "deviceModelName": "OROS-DEV2",
  "servicePackName": "OSCore/5.1",
  "version": "4.0.0-b2023-11-08T15:20:53.802Z",
  "description": "Open source Orchid web OS and browser",
  "main": "src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dev": "electron .",
    "browser": "electron . --chrome",
    "build": "electron-builder",
    "dist": "electron-builder --publish always",
    "build-linux-arm": "electron-builder --linux --armv7l",
    "build-linux-arm64": "electron-builder --linux --arm64",
    "build-linux-ia32": "electron-builder --linux --ia32",
    "build-linux-x64": "electron-builder --linux --x64",
    "build-win-arm64": "electron-builder --win --arm64",
    "build-win-ia32": "electron-builder --win --ia32",
    "build-win-x64": "electron-builder --win --x64",
    "build-mac-arm64": "electron-builder --mac --arm64",
    "build-mac-x64": "electron-builder --mac --x64"
  },
  "keywords": [
    "orchid",
    "openorchid",
    "orchidos",
    "games",
    "orchidgames",
    "orchidinteractive",
    "vividus",
    "vividusgames",
    "vividusengine",
    "vulpes",
    "vulpos",
    "aluminum",
    "aluminumos",
    "aluminium",
    "aluminiumos",
    "wos",
    "webos"
  ],
  "author": {
    "name": "MortCodesWeb, periyuh - Orchid Org.",
    "email": "mortcodesweb@proton.me"
  },
  "license": "GPL-2.0",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "archiver": "^5.3.1",
    "bluetooth": "^3.0.0",
    "child_process": "^1.0.2",
    "commander": "^11.0.0",
    "del": "^7.0.0",
    "discord-rpc": "^4.0.1",
    "dotenv": "^16.3.1",
    "download": "^8.0.0",
    "easy-volume": "^1.1.0",
    "electron-is-dev": "^2.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-protocols": "^1.0.4",
    "electron-reload": "^2.0.0-alpha.1",
    "env": "^0.0.2",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "google-translate-api": "^2.3.0",
    "http": "^0.0.1-security",
    "intl-locale": "^0.1.0",
    "is-windows": "^1.0.2",
    "mime": "^3.0.0",
    "mkdirp": "^3.0.1",
    "moment": "^2.29.4",
    "music-metadata-browser": "^2.5.10",
    "mv": "^2.1.1",
    "natural": "^6.5.0",
    "noble": "^1.9.1",
    "node-brightness": "^0.2.1",
    "node-wifi": "^2.0.16",
    "path": "^0.12.7",
    "rimraf": "^5.0.5",
    "running-apps": "^1.0.2",
    "sqlite3": "^5.1.6",
    "translate": "^2.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "csslint": "^1.0.5",
    "debug": "^4.3.4",
    "electron": "27.1.0",
    "electron-builder": "^24.4.0",
    "eslint": "^8.47.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "glob": "^10.3.4",
    "gulp": "^4.0.2",
    "jsdoc": "^4.0.2",
    "mini-css-extract-plugin": "^2.7.6",
    "svg-to-png": "^4.0.0",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "org.orchid.browser",
    "productName": "OrchidBrowser",
    "directories": {
      "output": "dist",
      "buildResources": "res"
    },
    "win": {
      "target": [
        "zip"
      ],
      "icon": "internal/branding/browser/browser_256.png",
      "publish": [
        "github"
      ]
    },
    "mac": {
      "target": [
        "zip",
        "dmg"
      ],
      "icon": "internal/branding/browser/browser_256.png",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": [
        "tar.gz",
        "deb"
      ],
      "icon": "internal/branding/browser/browser_256.png",
      "publish": [
        "github"
      ]
    },
    "files": [
      "internal",
      "src",
      ".env",
      ".gitignore",
      "AUTHORS",
      "index.js",
      "LICENSE",
      "package.json",
      "package-lock.json",
      "preload.js",
      "README.md"
    ],
    "extraFiles": [
      {
        "from": "customization/defaults",
        "to": "defaults"
      },
      {
        "from": "build_stage",
        "to": "webapps"
      },
      {
        "from": "webapps/shared/fonts",
        "to": "fonts"
      },
      {
        "from": "build/environment/production.env",
        "to": ".env"
      }
    ]
  }
}
